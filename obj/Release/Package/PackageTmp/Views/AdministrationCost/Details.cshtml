@model Alliance_for_Life.Models.AdminCosts

@{
    ViewBag.Title = "Details of Administrative Cost Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div>
  <hr />
  <dl>
      <dt class="col-md-2">
          Organization
      </dt>

      <dd>
          @Model.Subcontractor.OrgName
      </dd>
      <dt class="col-md-2">
          Region
      </dt>

      <dd>
          @Model.Region.GetDisplayName()
      </dd>
      <dt class="col-md-2">
          Month
      </dt>

      <dd>
          @Model.Month
      </dd>

      <dt class="col-md-2">
          Year
      </dt>

      <dd>
          @Model.Year
      </dd>

      <dt class="col-md-2">
          Salaries and Wages
      </dt>

      <dd>
          @Model.ASalandWages.ToString("C")
      </dd>

      <dt class="col-md-2">
          Employee Benefits
      </dt>

      <dd>
          @Model.AEmpBenefits.ToString("C")
      </dd>

      <dt class="col-md-2">
          Employee Travel
      </dt>

      <dd>
          @Model.AEmpTravel.ToString("C")
      </dd>

      <dt class="col-md-2">
          Employee Training
      </dt>

      <dd>
          @Model.AEmpTraining.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office Rent
      </dt>

      <dd>
          @Model.AOfficeRent.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office Utilities
      </dt>

      <dd>
          @Model.AOfficeUtilities.ToString("C")
      </dd>

      <dt class="col-md-2">
          Facility Insurance
      </dt>

      <dd>
          @Model.AFacilityIns.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office Supplies
      </dt>

      <dd>
          @Model.AOfficeSupplies.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office equipment/computers
      </dt>

      <dd>
          @Model.AEquipment.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office Communications
      </dt>

      <dd>
          @Model.AOfficeCommunications.ToString("C")
      </dd>

      <dt class="col-md-2">
          Office Maintenance
      </dt>

      <dd>
          @Model.AOfficeMaint.ToString("C")
      </dd>

      <dt class="col-md-2">
          Consulting Fees
      </dt>

      <dd>
          @Model.AConsulting.ToString("C")
      </dd>

      <dt class="col-md-2">
          Janitor Services
      </dt>

      <dd>
          @Model.AJanitorServices.ToString("C")
      </dd>

      <dt class="col-md-2">
          Depreciation
      </dt>

      <dd>
          @Model.ADepreciation.ToString("C")
      </dd>

      <dt class="col-md-2">
          Technical Support
      </dt>

      <dd>
          @Model.ATechSupport.ToString("C")
      </dd>

      <dt class="col-md-2">
          Security Services
      </dt>

      <dd>
          @Model.ASecurityServices.ToString("C")
      </dd>

      <dt class="col-md-2">
          @Model.AOtherInput
      </dt>

      <dd>
          @Model.AOther.ToString("C")
      </dd>

      <dt class="col-md-2">
          @Model.AOtherInput2
      </dt>

      <dd>
          @Model.AOther2.ToString("C")
      </dd>

      <dt class="col-md-2">
          @Model.AOtherInput3
      </dt>

      <dd>
          @Model.AOther3.ToString("C")
      </dd>

      <dt class="col-md-2">
          Total Costs
      </dt>

      <dd>
          @Model.ATotCosts.ToString("C")
      </dd>

  </dl>
</div>
<p>
    @if (!User.IsInRole("Admin"))
    {
        @Html.ActionLink("Delete |", "Delete", new { id = Model.AdminCostId });
}
    @Html.ActionLink("Back to List", "Index")
</p>
